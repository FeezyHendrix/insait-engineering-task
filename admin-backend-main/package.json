{
  "name": "backend",
  "version": "0.7.3",
  "main": "server.js",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --detectOpenHandles",
    "dev": "nodemon --ignore node_modules --exec ts-node src/server.ts",
    "prisma:generate": "cd src/prisma && npx prisma generate",
    "prisma:seed": "npx prisma db seed",
    "prestart": "cd src/prisma && npx prisma migrate deploy",
    "start": "node src/server.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.688.0",
    "@aws-sdk/s3-request-presigner": "^3.688.0",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^8.2.0",
    "@prisma/client": "^6.2.1",
    "@sentry/node": "^8.49.0",
    "@sentry/profiling-node": "^8.49.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.2",
    "axios-retry": "^4.4.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dd-trace": "^5.51.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pino": "^8.16.1",
    "pino-http": "^10.4.0",
    "pino-pretty": "^10.2.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "twilio": "^4.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/socket.io-client": "^3.0.0",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.10",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "msw": "^2.3.5",
    "nodemon": "^3.0.1",
    "prisma": "^6.2.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.2"
  },
